{
  "name": "tsinghua",
  "description": "Environment used in testing the upstream cookbooks and reference Chef repository",
  "cookbook_versions": {
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
  },
  "override_attributes": {
    "memcached": {
      "bind_interface": "eth0"
    },
    "mysql": {
      "server_root_password": "test",
      "server_debian_password": "root",
      "server_repl_password": "root",
      "allow_remote_root": true,
      "root_network_acl": "%"
    },
    "haproxy": {
      "incoming_address": "10.1.1.250"
    },
    "keepalived": {
      "vip": {
        "ipaddress": "10.1.1.250",
        "interface": "eth0"
      }
    },
    "openstack": {
      "auth": {
        "validate_certs": false
      },
      "block-storage": {
        "syslog": {
          "use": false
        },
        "api": {
          "ratelimit": "False"
        },
        "debug": false
      },
      "dashboard": {
        "use_ssl": "false"
      },
      "compute": {
        "syslog": {
          "use": false
        },
        "libvirt": {
          "bind_interface": "eth0"
        },
        "novnc_proxy": {
          "bind_interface": "eth0"
        },
        "ratelimit": {
          "api": {
            "enabled": false
          },
          "volume": {
            "enabled": false
          }
        },
        "network": {
          "service_type": "neutron"
        }
      },
      "network": {
        "verbose": "False",
        "debug": "False",
        "service_plugins": [
          "router"
        ],
        "ml2": {
          "type_drivers": "local,flat,vlan,gre",
          "tenant_network_types": "vlan,gre",
          "network_vlan_ranges": "physnet1:1000:2999",
          "enable_security_group": "True"
        },
        "openvswitch": {
          "tenant_network_type": "vlan",
          "network_vlan_ranges": "physnet1:1000:2999",
          "bridge_mappings": "physnet1:br-eth5",
          "fw_driver": "neutron.agent.firewall.NoopFirewallDriver"
        }
      },
      "db": {
        "bind_interface": "eth0",
        "compute": {
          "host": "10.1.0.205"
        },
        "identity": {
          "host": "10.1.0.205"
        },
        "image": {
          "host": "10.1.0.205"
        },
        "network": {
          "host": "10.1.0.205"
        },
        "volume": {
          "host": "10.1.0.205"
        },
        "dashboard": {
          "host": "10.1.0.205"
        }
      },
      "developer_mode": true,
      "endpoints": {
        "db": {
          "host": "10.1.0.205"
        },
        "mq": {
          "host": "10.1.0.205"
        },
        "compute-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "8774",
          "path": "/v2/%(tenant_id)s"
        },
        "compute-api-bind": {
          "bind_interface": "eth0"
        },
        "compute-ec2-admin": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "8773",
          "path": "/services/Admin"
        },
        "compute-ec2-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "8773",
          "path": "/services/Cloud"
        },
        "compute-novnc": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "6080",
          "path": "/vnc_auto.html"
        },
        "compute-novnc-bind": {
          "bind_interface": "eth0"
        },
        "compute-vnc-bind": {
          "bind_interface": "eth0"
        },
        "image-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "9292",
          "path": "/v2"
        },
        "image-api-bind": {
          "bind_interface": "eth0"
        },
        "image-registry": {
          "bind_interface": "eth0"
        },
        "image-registry-bind": {
          "bind_interface": "eth0"
        },
        "identity-bind": {
          "bind_interface": "eth0"
        },
        "identity-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "5000",
          "path": "/v2.0"
        },
        "identity-admin": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "35357",
          "path": "/v2.0"
        },
        "block-storage-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "8776",
          "path": "/v1/%(tenant_id)s"
        },
        "block-storage-api-bind": {
          "bind_interface": "eth0"
        },
        "telemetry-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "8777",
          "path": "/v1"
        },
        "network-api": {
          "host": "10.1.1.250",
          "scheme": "http",
          "port": "9696",
          "path": ""
        },
        "network-api-bind": {
          "bind_interface": "eth0"
        },
        "network-openvswitch": {
          "bind_interface": "eth5"
        }
      },
      "identity": {
        "admin_user": "admin",
        "bind_interface": "eth0",
        "catalog": {
          "backend": "sql"
        },
        "debug": false,
        "roles": [
          "admin",
          "member"
        ],
        "syslog": {
          "use": false
        },
        "tenants": [
          "admin",
          "service"
        ],
        "users": {
          "admin": {
            "password": "admin",
            "default_tenant": "admin",
            "roles": {
              "admin": [
                "admin"
              ]
            }
          }
        }
      },
      "image": {
        "api": {
          "bind_interface": "eth0"
        },
        "debug": false,
        "registry": {
          "bind_interface": "eth0"
        },
        "syslog": {
          "use": false
        },
        "upload_image": {
          "cirros": "http://download.cirros-cloud.net/0.3.2/cirros-0.3.2-x86_64-disk.img"
        },
        "upload_images": [
          "cirros"
        ]
      },
      "mq": {
        "user": "guest",
        "password": "test",
        "vhost": "/nova",
        "network": {
          "service_type": "rabbitmq"
        }
      }
    }
  }
}
